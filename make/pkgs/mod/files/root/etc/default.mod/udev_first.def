CAPTION='Freetz: udev-first'
DESCRIPTION="$(lang de:"Diese Regeln werden am Anfang von UDEV ausgef&uuml;hrt" en:"rules are executed at the beginning of udev").<br>
$(lang de:"Daten auslesen: 'udevadm monitor --udev' & Ger&auml;t verbinden & 'udevadm info -ap /PFAD/VOM/MONITOR'" en:"To get data: 'udevadm monitor --udev' & attach device & 'udevadm info -ap /PATH/FROM/MONITOR'")"

CONFIG_FILE='/tmp/flash/mod/udev-first.rules'
