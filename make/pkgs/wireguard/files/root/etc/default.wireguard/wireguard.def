CAPTION="$(lang de:"WireGuard Konfiguration" en:"WireGuard configuration")"
DESCRIPTION="$(lang de:"Hier muss die entsprechende WireGuard Konfiguration eingetragen werden." en:"The WireGuard configuration has to be inserted here.")"

CONFIG_FILE='/tmp/flash/wireguard/wireguard.conf'
CONFIG_SAVE='modsave flash; [ "$(/mod/etc/init.d/rc.wireguard status)" == "running" ] && /mod/etc/init.d/rc.wireguard restart'
CONFIG_TYPE='text'

