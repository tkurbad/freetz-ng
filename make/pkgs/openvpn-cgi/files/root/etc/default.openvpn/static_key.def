CAPTION='Static Key (OpenVPN)'
DESCRIPTION="$(lang de:"Automatisch erzeugter Schl&uuml;ssel. Wird als statischer Schl&uuml;ssel oder als TLS-Schl&uuml;ssel auf der/den Gegenseite(n) ben&ouml;tigt." en:"Automaticly generated. Required as static key or as TLS-key on the remote site(s).")"

CONFIG_FILE='/tmp/flash/openvpn/static.key'
CONFIG_SAVE='modsave flash; if [ ! -z "$(pidof openvpn)" ]; then echo ""; /mod/etc/init.d/rc.openvpn reload; fi'
CONFIG_TYPE='text'

